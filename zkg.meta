[package]
description = A HTTP2 protocol analyzer for the Zeek NSM.
tags = zeek plugin, protocol analyzer, http2, intel
script_dir = scripts
depends =
    zeek >=3.0.0
external_depends =
    libnghttp2>=1.11.0
    libbrotlidec>=1.0.0
build_command = ./configure --zeek-dist=%(zeek_dist)s && make
test_command = make test
